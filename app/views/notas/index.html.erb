<%- model_class = Nota -%>
<div class="page-header">
  <h1>Notas</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>Items</th>
      <th>Num documento recibe</th>
      <th>Unidad</th>
      <th>Tipo</th>
      <th>Created at</th>
      <th>Actions</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @notas %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nueva Nota', new_nota_path, :remote => true %></td>
    </tr>
  </tfoot>
</table>
