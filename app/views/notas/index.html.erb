<%- model_class = Nota -%>
<div class="page-header">
<h1>Notas</h1>
</div>
<div class="well" id="tablas">

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Items</th>
      <th>Numero de documento (Recibe)</th>
      <th>Unidad</th>
      <th>Tipo</th>
      <th>Fecha de Creacion</th>
    </tr>
  </thead>
  <tbody>
     <%= render @notas %>
  </tbody>
</table>
</div>
<%= link_to 'Nueva Nota', new_nota_path, :remote => true, :class => 'btn btn-primary' %>