<%- model_class = Registrocompra -%> 
<div class="page-header">
<h1>Registro De Compras</h1>
</div>
<div class="well" id="tablas">

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Descripcion Del Producto</th>
      <th>Valor Total</th>
      <th>Valor Unidad</th>
      <th>Fecha de Creacion</th>
    </tr>
  </thead>
  <tbody>
     <%= render @registrocompras %>
  </tbody>
</table>
</div>
<%= link_to 'Nuevo Registro', new_registrocompra_path, :remote => true, :class => 'btn btn-primary' %>
